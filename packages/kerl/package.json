    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prepare": "tsc",
        "test": "tsc && nyc ava",
        "test-ci": "nyc ava",
        "lint": "tslint --project ."
    },
    "contributors": [
        "Dominik Schiener <dom@iota.org> (https://iota.org)",
        "Edward Greve <edward@iota.org> (https://github.com/anyong)",
